{
    "name": "kaitoj/terms-gen",
    "description": "Laravel EU GDPR complient cookie, privacy nd terms generator with cookie consent",
    "license": "MIT",
    "homepage": "https://github.com/kaitoj/terms-gen",
    "authors": [
        {
            "name": "Kaito Jacobson",
            "email": "kaito.jacobson@gmail.com",
            "homepage": "https://www.kai-dev.xyz",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": ">=7.15",
        "illuminate/translation": "~5.8.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench":  "~3.8.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Kaitoj\\Termsgen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kaitoj\\Termsgen\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kaitoj\\Termsgen\\TermsgenServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
